{
  "cli": {
    "version": ">= 16.3.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "release": {
      "distribution": "store"
    }
  },
  "submit": {
    "production": {}
  }
}
