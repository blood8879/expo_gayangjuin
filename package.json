{
  "name": "expo-gayangjuin",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:ci": "jest --coverage --ci --collectCoverageFrom='**/*.{ts,tsx}' --collectCoverageFrom='!**/*.d.ts' --collectCoverageFrom='!**/node_modules/**' --collectCoverageFrom='!**/coverage/**' --collectCoverageFrom='!**/__tests__/**' --collectCoverageFrom='!**/*.test.{ts,tsx}' --collectCoverageFrom='!**/*.spec.{ts,tsx}' --collectCoverageFrom='!**/babel.config.js' --collectCoverageFrom='!**/jest.config.js' --collectCoverageFrom='!**/tailwind.config.js' --collectCoverageFrom='!**/app.config.js' --collectCoverageFrom='!**/metro.config.js' --collectCoverageFrom='!**/expo-env.d.ts' --collectCoverageFrom='!**/global.d.ts' --collectCoverageFrom='!**/scripts/**' --collectCoverageFrom='!**/assets/**' --collectCoverageFrom='!**/app.json' --collectCoverageFrom='!**/eas.json' --coverageThreshold='{\"global\":{\"branches\":99,\"functions\":99,\"lines\":99,\"statements\":99}}'  --coverageReporters=text-lcov --coverageReporters=json-summary",
    "test:coverage": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":99,\"functions\":99,\"lines\":99,\"statements\":99}}'  --coverageReporters=text --coverageReporters=html --coverageReporters=json-summary",
    "test:badges": "jest-coverage-badges",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@supabase/.*|@tanstack/.*)"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/__tests__/**",
      "!**/*.test.{ts,tsx}",
      "!**/*.spec.{ts,tsx}",
      "!**/babel.config.js",
      "!**/jest.config.js",
      "!**/tailwind.config.js",
      "!**/app.config.js",
      "!**/metro.config.js",
      "!**/expo-env.d.ts",
      "!**/global.d.ts",
      "!**/scripts/**",
      "!**/assets/**",
      "!**/app.json",
      "!**/eas.json"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 99,
        "functions": 99,
        "lines": 99,
        "statements": 99
      }
    },
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-google-signin/google-signin": "^13.2.0",
    "@react-native-kakao/core": "^2.3.0",
    "@react-native-kakao/user": "^2.3.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.74.4",
    "base64-arraybuffer": "^1.0.2",
    "date-fns": "^4.1.0",
    "expo": "~52.0.46",
    "expo-auth-session": "~6.0.3",
    "expo-blur": "~14.0.3",
    "expo-build-properties": "~0.13.2",
    "expo-constants": "~17.0.8",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image-picker": "~16.0.6",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-modules-core": "~2.2.3",
    "expo-router": "~4.0.20",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.9",
    "expo-web-browser": "~14.0.2",
    "nativewind": "^4.1.23",
    "query-string": "^7.1.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-popup-menu": "^0.17.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "tailwindcss": "^3.4.17",
    "tamagui": "^1.126.1",
    "web-streams-polyfill": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "jest": "^29.2.1",
    "jest-coverage-badges": "^1.1.2",
    "jest-expo": "~52.0.6",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
  "private": true
}
